<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="dotnet" arguments="run -c Production --no-build" forwardWindowsAuthToken="false" stdoutLogEnabled="true" stdoutLogFile="C:\inetpub\logs\stdout.log">
      <environmentVariables>
        <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Production" />
        <environmentVariable name="DOTNET_CLI_TELEMETRY_OPTOUT" value="1"/>
      </environmentVariables>
    </aspNetCore>
  </system.webServer>
</configuration>